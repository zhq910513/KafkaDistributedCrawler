{
  "dashboard": {
    "id": null,
    "title": "Consul Services Monitoring",
    "tags": [],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "type": "graph",
        "title": "Kafka Messages In",
        "targets": [
          {
            "expr": "rate(kafka_server_brokertopicmetrics_messages_in_total[5m])",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Redis Memory Usage",
        "targets": [
          {
            "expr": "redis_memory_usage_bytes",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "bytes"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Consul Health Check Status",
        "targets": [
          {
            "expr": "consul_health_service_status{status='passing'}",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "xaxis": {
          "mode": "time"
        },
        "yaxes": [
          {
            "format": "short"
          }
        ]
      }
    ]
  },
  "overwrite": true
}
